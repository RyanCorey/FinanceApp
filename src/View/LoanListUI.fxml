<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="382.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoanListControl">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Loans" />
        <TableView fx:id="loanTable" layoutX="14.0" layoutY="38.0" prefHeight="272.0" prefWidth="532.0">
            <columns>
                <TableColumn fx:id="loanNumber" prefWidth="75.0" text="Loan #" />
                <TableColumn fx:id="loanAmount" prefWidth="129.0" text="Amount" />
                <TableColumn fx:id="loanStatus" prefWidth="156.0" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="viewLoanButton" layoutX="368.0" layoutY="331.0" mnemonicParsing="false" onAction="#viewLoanClicked" text="View Loan" />
        <Button fx:id="returnToMenuButton" layoutX="131.0" layoutY="331.0" mnemonicParsing="false" onAction="#returnToMenuButtonClicked" text="Return To Menu" />
    </children>
</AnchorPane>
