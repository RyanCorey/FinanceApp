<AnchorPane id="AnchorPane" prefHeight="185.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginControl">
    <children>
        <TextField fx:id="usernameText" layoutX="104.0" layoutY="22.0" prefHeight="25.0" prefWidth="171.0" />
        <PasswordField fx:id="passwordText" layoutX="104.0" layoutY="54.0" prefHeight="25.0" prefWidth="171.0" />
        <Button fx:id="loginButton" defaultButton="true" layoutX="172.0" layoutY="93.0" mnemonicParsing="false" onAction="#authenticate" prefHeight="25.0" prefWidth="103.0" text="Log In" />
        <Label fx:id="usernameLabel" alignment="CENTER_RIGHT" layoutX="21.0" layoutY="27.0" prefHeight="15.0" prefWidth="70.0" text="Username" textAlignment="RIGHT" />
        <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" layoutX="21.0" layoutY="59.0" prefHeight="15.0" prefWidth="70.0" text="Password" />
        <Separator layoutX="10.0" layoutY="130.0" prefHeight="15.0" prefWidth="265.0" />
        <Button fx:id="createAccountButton" layoutX="10.0" layoutY="147.0" mnemonicParsing="false" onAction="#createAccount" text="Create Account" />
        <Button layoutX="136.0" layoutY="147.0" mnemonicParsing="false" onAction="#help" prefHeight="25.0" prefWidth="60.0" text="Help" />
        <Button fx:id="exitButton" layoutX="205.0" layoutY="147.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Exit" />
    </children>
</AnchorPane>
